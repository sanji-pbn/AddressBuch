package de.ems.addressbuch.model;

public class Kontakt {

	private String pk;

	private String emailOrPhone;
	
	private String person_fk;
	
	public Kontakt() {
		
	}
	
	public Kontakt(String emailOrPhone, String personFk) {
		super();
		this.emailOrPhone = emailOrPhone;
		this.personFk = personFk;
	}

	public String getPk() {
		return pk;
	}

	public void setPk(String pk) {
		this.pk = pk;
	}

	public String getEmailOrPhone() {
		return emailOrPhone;
	}

	public void setEmailOrPhone(String emailOrPhone) {
		this.emailOrPhone = emailOrPhone;
	}


	public String getPerson_fk() {
		return person_fk;
	}
	public void setPerson_fk(String person_fk) {
		this.person_fk = person_fk;
	}

	@Override
	public String toString() {
		return "Kontakt [pk=" + pk + ", emailOrPhone=" + emailOrPhone + ", personFk=" + personFk + "]";
	}
	
	
}
